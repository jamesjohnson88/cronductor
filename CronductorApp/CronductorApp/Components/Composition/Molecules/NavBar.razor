@using CronductorApp.Components.Composition.Atoms

<!-- Navigation Bar -->
<nav class="navbar" role="navigation">
    @foreach (var item in NavigationItems)
    {
        <NavBarItem Title="@item.Key" Link="@item.Value" />
    }
</nav>

@code {
    [Parameter]
    public required Dictionary<string, string> NavigationItems { get; set; }
}